<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white" >

    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/title_height"
        android:background="@color/all_page_bg" >

        <TextView
            style="@style/textview_title"
            android:text="财富" />
    </RelativeLayout>

    <View
        android:id="@+id/v_unlie"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/top"
        android:background="#3C6AA3" />

    <LinearLayout
        android:id="@+id/yue_fanyong_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_unlie"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/balance_layout"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="#40618C" >

            <ImageView
                android:id="@+id/balance"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:src="@drawable/yue" />

            <TextView
                android:id="@+id/mobileTopUpLabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/balance"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="2dp"
                android:text="余额"
                android:textColor="@color/whilte"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/main_t1_pay0_amt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/mobileTopUpLabel"
                android:layout_centerHorizontal="true"
                android:text="120.00"
                android:textColor="@color/whilte"
                android:textSize="15sp" />
        </RelativeLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#3C6AA3" />

        <RelativeLayout
            android:id="@+id/rebate_layout"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="#40618C" >

            <ImageView
                android:id="@+id/fanyong"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:src="@drawable/fanyong" />

            <TextView
                android:id="@+id/tv_fanyong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fanyong"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="2dp"
                android:text="返佣"
                android:textColor="@color/whilte"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/main_t1_rate_amt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_fanyong"
                android:layout_centerHorizontal="true"
                android:text="120.00"
                android:textColor="@color/whilte"
                android:textSize="15sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/full"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/yue_fanyong_layout"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/one"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#000000"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/weixint1_layout"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_weixint1"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_weixint1" />

                <TextView
                    android:id="@+id/tv_yue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_weixint1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="微信T1"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/weixint0_layout"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_weixint0"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_weixint0" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_weixint0"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="微信T0"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <FrameLayout
                    android:id="@+id/jifen_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:background="@drawable/icon_new" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent" >

                        <ImageView
                            android:id="@+id/img_jifen"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="20dp"
                            android:src="@drawable/c_jifen" />

                        <TextView
                            android:id="@+id/tv_jifen"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/img_jifen"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="2dp"
                            android:text="积分"
                            android:textColor="#959597"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/main_t1_jf_amt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/tv_jifen"
                            android:layout_centerHorizontal="true"
                            android:textColor="#959597"
                            android:textSize="15sp" />
                    </RelativeLayout>
                </FrameLayout>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/two"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#000000"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/zhuanzhang_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_zhuanzhang"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_zhuanzhang" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_zhuanzhang"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="转账"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/yiyuanduobao_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_yiyuanduobao"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_yiyuanduobao" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_yiyuanduobao"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="一元夺宝"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/xinyongkagl_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_xinyongkagl"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_xinyongkagl" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_xinyongkagl"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="信用卡攻略"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/three"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#000000"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/liuliang_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_liuliang"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_liuliang" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_liuliang"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="流量包购买"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <FrameLayout
                    android:id="@+id/jrchaoshi_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.0"
                    android:background="@drawable/icon_new" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <ImageView
                            android:id="@+id/img_jinrongchangshi"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="25dp"
                            android:src="@drawable/c_jinrongchangshi" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/img_jinrongchangshi"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="2dp"
                            android:text="金融超市"
                            android:textColor="#959597"
                            android:textSize="15sp" />
                    </RelativeLayout>
                </FrameLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/shangcheng_layout"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@drawable/main_border2" >

                <ImageView
                    android:id="@+id/img_shangcheng"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:src="@drawable/c_shangcheng" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_shangcheng"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="商城"
                    android:textColor="#959597"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#000000"
            android:orientation="horizontal" >

            <cn.qmpos.view.ImageCycleView
                android:id="@+id/ad_view"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/transtantbg_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#60000000"
        android:visibility="gone" >
    </RelativeLayout>

</RelativeLayout>