<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.action.extension" value="do,"/>

    <package name="default" extends="json-default">
        <action name="shoppay.*" class="com.weixin.action.PayAction" method="prePay">
        </action>
        <action name="jjzzjjzz.*" class="com.weixin.action.WebpagePayAction" method="scanPay">
            <result name="scanPay">../weixin/scanpay.jsp</result>
        </action>
        <action name="WebpagePayAction!*" class="com.weixin.action.WebpagePayAction" method="{1}">
            <result name="scanPay">../weixin/scanpay.jsp</result>
        </action>
    </package>
</struts>