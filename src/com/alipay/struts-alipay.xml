<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="alipay" namespace="/alipay" extends="json-default">
        <global-results>
            <result name="ajaxActionCompleted" type="json">
                <param name="root">ajaxActionResult</param>
            </result>
        </global-results>

        <action name="Pay!*" class="com.alipay.action.PayAction"  method="{1}">
        </action>
        <action name="Callback!*" class="com.alipay.action.CallbackAction" method="{1}">
        </action>
    </package>
</struts>